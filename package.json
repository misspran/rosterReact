{
  "name": "players-react-skeleton",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build-client": "webpack",
    "build-client-watch": "webpack -w",
    "build-semantic": "cd src/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && yarn run build-semantic && gulp watch",
    "start": "cross-env NODE_ENV=development parcel src/index.html -p 3000",
    "build": "cross-env NODE_ENV=production parcel build index.html --public-url && webpack",
    "e2e": "cypress open",
    "lint:js": "eslint src/**/**/*.js src/**/**/*.jsx tests/**/**/*.js",
    "lint:style": "stylelint src/**/**/*.css src/**/**/*.scss"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-react-app": "^3.1.2",
    "cross-env": "^5.2.0",
    "cypress": "^3.0.2",
    "gulp": "^4.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "parcel-bundler": "^1.9.7",
    "path": "^0.12.7",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.82.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "prettier": "^1.13.7",
    "prettierrc": "0.0.0-5",
    "semantic-ui": "^2.3.3",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  }
}
